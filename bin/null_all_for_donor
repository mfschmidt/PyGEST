#!/usr/bin/env bash

if [ "$PYGEST_DATA" == "" ]
then
    PYGEST_DATA="$HOME/ge_data"
fi

CMD="singularity run --bind PYGEST_DATA:/data"
IMG="$HOME/bin/pygest.simg"

for H in L R A
do
    for S in cortex subcortical all
    do
        for M in min max
        do
            $CMD $IMG pushr $1 $H -s $S -m $M -n $2 --shuffle --seed $3
        done
    done
done
